@*Create Customer*@
@model Customer

<form method="post" style="color:antiquewhite;font-weight:bold;">
    <div>
        Nom de famille :
        <br />
        <input asp-for="LastName" />
    </div>
    <div>
        Prénom :
        <br />
        <input asp-for="FirstName" />
    </div>
    <div>
        Date de naissance :
        <br />
        <input asp-for="BirthDate" />
    </div>
    <div>
        <button type="submit" style="background-color:green;color:white;">Créer</button>
    </div>
</form>

@{
}
